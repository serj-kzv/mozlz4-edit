{
  "name": "ext-app",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "build:ba": "ng build ba-app --base-href ./",
    "build:bg": "ng build bg-app --base-href ./",
    "build:co": "ng build co-app --base-href ./",
    "build:op": "ng build op-app --base-href ./",
    "build:pa": "ng build pa-app --base-href ./",
    "build:sb": "ng build sb-app --base-href ./",
    "build:dp": "ng build dp-app --base-href ./",
    "build:nt": "ng build nt-app --base-href ./",
    "build:dpanel": "ng build dpanel-app --base-href ./",
    "watch:ba": "ng build ba-app --base-href ./ --watch",
    "watch:bg": "ng build bg-app --base-href ./ --watch",
    "watch:co": "ng build co-app --base-href ./ --watch",
    "watch:op": "ng build op-app --base-href ./ --watch",
    "watch:pa": "ng build pa-app --base-href ./ --watch",
    "watch:sb": "ng build sb-app --base-href ./ --watch",
    "watch:dp": "ng build dp-app --base-href ./ --watch",
    "watch:nt": "ng build nt-app --base-href ./ --watch",
    "watch:dpanel": "ng build dpanel-app --base-href ./ --watch",
    "prod:ba": "ng build ba-app --source-map=false --outputHashing=none --base-href ./ --prod",
    "prod:bg": "ng build bg-app --source-map=false --outputHashing=none --base-href ./ --prod",
    "prod:co": "ng build co-app --source-map=false --outputHashing=none --base-href ./ --prod",
    "prod:op": "ng build op-app --source-map=false --outputHashing=none --base-href ./ --prod",
    "prod:pa": "ng build pa-app --source-map=false --outputHashing=none --base-href ./ --prod",
    "prod:sb": "ng build sb-app --source-map=false --outputHashing=none --base-href ./ --prod",
    "prod:dp": "ng build dp-app --source-map=false --outputHashing=none --base-href ./ --prod",
    "prod:nt": "ng build nt-app --source-map=false --outputHashing=none --base-href ./ --prod",
    "prod:dpanel": "ng build dpanel-app --source-map=false --outputHashing=none --base-href ./ --prod",
    "copy-manifest": "node copy-manifest.js",
    "build": "npm run copy-manifest && run-p --aggregate-output -l -c build:*",
    "watch": "npm run copy-manifest && run-p --aggregate-output -l -c watch:*",
    "prod": "npm run copy-manifest && run-p --aggregate-output -l -c prod:*"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.1.4",
    "@angular/common": "^10.1.4",
    "@angular/compiler": "^10.1.4",
    "@angular/core": "^10.1.4",
    "@angular/forms": "^10.1.4",
    "@angular/platform-browser": "^10.1.4",
    "@angular/platform-browser-dynamic": "^10.1.4",
    "@angular/router": "^10.1.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.4",
    "@angular/cli": "^10.1.4",
    "@angular/compiler-cli": "^10.1.4",
    "@types/node": "^12.12.62",
    "fs-extra": "^9.0.1",
    "ng-packagr": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.0.3"
  }
}
